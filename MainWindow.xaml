<Window x:Class="Bacterium.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="534" Width="583" Background="White" ResizeMode="CanMinimize" WindowStartupLocation="Manual">
    <Grid x:Name="RootGrid">
        <Image x:Name="loadButton" HorizontalAlignment="Left" Height="34" Margin="444,461,0,0" VerticalAlignment="Top" Width="34" MouseLeftButtonDown="loadButton_MouseLeftButtonDown">
        </Image>
        <Label x:Name="label1" Foreground="White" Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Foreground="White" Content="Label" HorizontalAlignment="Left" Margin="529,10,0,0" VerticalAlignment="Top"/>
        <Image x:Name="imageFlag1" HorizontalAlignment="Left" Height="36" Margin="10,36,0,0" VerticalAlignment="Top" Width="38"/>
        <Image x:Name="imageFlag2" HorizontalAlignment="Left" Height="36" Margin="529,36,0,0" VerticalAlignment="Top" Width="38"/>
        <Image x:Name="saveButton" HorizontalAlignment="Left" Height="34" Margin="483,461,0,0" VerticalAlignment="Top" Width="34" MouseDown="saveButton_MouseDown"/>
        <Image x:Name="resetButton" HorizontalAlignment="Left" Height="34" Margin="529,461,0,0" VerticalAlignment="Top" Width="34" MouseDown="resetButton_MouseDown"/>
        <Canvas x:Name="PointGrid" MouseLeftButtonDown="PointGrid_MouseLeftButtonDown" MouseLeftButtonUp="PointGrid_MouseLeftButtonUp" MouseMove="PointGrid_MouseMove">
        </Canvas>
    </Grid>
</Window>
